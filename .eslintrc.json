{
	"env": {
		"es2021": true,
		"node": true,
		"jest": true
	},
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"plugin:jest/all"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint",
		"jest"
	],
	"settings": {
		"jest": {
			"version": 26
		}
	}
}